<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.healthok.ui.main.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.healthok.ui.main.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/telemedicineFragment"
        android:name="com.example.healthok.ui.main.TelemedicineFragment"
        android:label="fragment_telemedicine_fragment"
        tools:layout="@layout/fragment_telemedicine_fragment" />
    <fragment
        android:id="@+id/treatmentFragment"
        android:name="com.example.healthok.ui.main.TreatmentFragment"
        android:label="fragment_treatment"
        tools:layout="@layout/fragment_treatment" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.healthok.ui.authentication.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
</navigation>